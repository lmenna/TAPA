<style type="text/css">

body { font: 16px Arial;}

/* Style the lines by removing the fill and applying a stroke */
/* Color palette
* stroke: #A9A9A9; // darkgrey
* stroke: #4682b4; // steelblue
* stroke: #A9A9A9; // darkgrey
*/
.lineForPrices {
    fill: none;
    stroke: #000000; // black
    stroke-width: 1px;
}

/* Style the lines by removing the fill and applying a stroke */
.lineForTransactions {
    fill: none;
    stroke: #4682b4; // steelblue
    stroke-width: 1px;
}

.y-axis-label {
  font-family: 'Tangerine', serif;
}

.axis text {
  font-family: 'Tangerine', serif;
}

.title text {
  font-size:   '26px';
  text-decoration: 'underline';
  font-family: 'Tangerine', serif;
}

.overlay {
  fill: none;
  pointer-events: all;
}

/* Style the dots by assigning a fill and stroke */
.dot {
    fill: #ffab00;
    stroke: #fff;
}

.focus circle {
  fill: none;
  stroke: steelblue;
}
</style>

